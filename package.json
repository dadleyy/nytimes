{
  "name": "news",
  "version": "0.0.1",
  "description": "ny time new aggregator built w/ react",
  "config": {
    "dist_dir": "./dist/news",
    "asset_dist_dir": "./dist/news/assets",
    "main_js_src": "./dist/news/assets/main.js"
  },
  "scripts": {
    "build:html": "pug -o $npm_package_config_dist_dir ./src/index.pug",
    "build:scripts": "tsc -p ./tsconfig.json -outDir $npm_package_config_asset_dist_dir",
    "build:optimize": "BUNDLED=true npm run build:html && webpack --config=./webpack.build.js",
    "build": "npm run build:scripts && npm run build:html",
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "test": "karma start --single-run=true",
    "start": "live-server --middleware=spa-ignore-assets ./dist/news"
  },
  "author": "Danny Hadley <dadleyy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "16.0.6",
    "@types/react-dom": "15.5.4",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.0.7",
    "ajv": "^5.2.3",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-plugin-transform-es2015-modules-amd": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "7.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.4",
    "karma-narrow-reporter": "2.0.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-requirejs": "1.1.0",
    "karma-tslint": "1.0.2",
    "karma-typescript": "3.0.7",
    "live-server": "^1.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "requirejs": "^2.3.5",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2"
  }
}
